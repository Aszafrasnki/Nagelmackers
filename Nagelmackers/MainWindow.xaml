            <ComboBox x:Name="DestinationFrom" Canvas.Left="83" Text="Z" Canvas.Top="165" Background="#FFDE7824" Width="373" Height="74" FontSize="36" HorizontalAlignment="Center" VerticalAlignment="Center" Cursor="Pen" Padding="20,15,0,0" ScrollViewer.CanContentScroll="True" BorderBrush="#FFDE7824" DropDownOpened="DestinationFrom_DropDownOpened" DropDownClosed="DestinationFrom_DropDownClosed"/>
            <Button x:Name="DestinationSwitcher" Content="↔" Background="{x:Null}" BorderBrush="{x:Null}" FontSize="36" Width="54" Height="74" Canvas.Left="461" Canvas.Top="165" FontStretch="Normal" HorizontalAlignment="Left" VerticalAlignment="Center" Click="DestinationSwitcher_Click" Cursor="Hand"/>
